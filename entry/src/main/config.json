{
  "app": {
    "bundleName": "es.espinr.litewearabletesting",
    "vendor": "Martin",
    "version": {
      "code": 1,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {
  },
  "module": {
    "deviceType": [
      "liteWearable"
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.READ_HEALTH_DATA",
        "reason": "Read your HR for good",
        "usedScene": {
          "ability": [
            "default"
          ],
          "when": "always"
        }
      }
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "name": "default",
        "icon": "$media:icon",
        "label": "$string:entry_MainAbility",
        "type": "page"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "default"
      }
    ]
  }
}